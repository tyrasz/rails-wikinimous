<h1>Articles</h1>
<table>
  <thead>
    <tr>
      <td>Title</td>
      <td>Content</td>
      <td>Link to article</td>
    </tr>
  </thead>
  <tbody>
    <% @articles.each do |article| %>
    <tr>
      <td><%= article.title %></td>
      <td><%= article.content %></td>
       <td><%= link_to "Go to article", article %></td>
    </tr>
    <% end %>
  </tbody>
</table>
